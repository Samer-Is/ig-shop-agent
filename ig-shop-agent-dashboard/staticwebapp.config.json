{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*.{png,jpg,gif,ico,svg,js,css}"]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
} 